{
    "name": "getkevin/kevin-prestashop",
    "description": "PrestaShop module for kevin. client.",
    "type": "prestashop-module",
    "version": "1.7.3",
    "license": "MIT",
    "authors": [
        {
            "name": "kevin.",
            "email": "info@getkevin.eu"
        }
    ],
    "support": {
        "email": "support@getkevin.eu"
    },
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "getkevin/kevin-php": "^0.9.0"
    },
    "config": {
        "archive-dir": ".",
        "archive-format": "zip",
        "prepend-autoloader": false
    },
    "scripts": {
        "pre-archive-cmd": "composer update"
    },
    "archive": {
        "name": "kevin",
        "exclude": [
            "!vendor/*",
            ".*",
            "composer.lock"
        ]
    }
}
